all: small large huge

small:
	$(MAKE) run -C ./005m_008x032
	$(MAKE) run -C ./005m_008x016
	$(MAKE) run -C ./005m_004x016
	$(MAKE) run -C ./005m_004x008
	$(MAKE) run -C ./005m_002x008
	$(MAKE) run -C ./005m_002x004
	$(MAKE) run -C ./005m_001x004
	$(MAKE) run -C ./005m_001x002

large:
	$(MAKE) run -C ./005m_096x384
	$(MAKE) run -C ./005m_096x192
	$(MAKE) run -C ./005m_064x256
	$(MAKE) run -C ./005m_064x128
	$(MAKE) run -C ./005m_032x128
	$(MAKE) run -C ./005m_032x064
	$(MAKE) run -C ./005m_016x064
	$(MAKE) run -C ./005m_016x032

huge:
	$(MAKE) run -C ./005m_144x576
	$(MAKE) run -C ./005m_144x288

ex:
	$(MAKE) run -C ./005m_032x128
	$(MAKE) run -C ./005m_008x032
	$(MAKE) run -C ./005m_002x008
